  <Properties>
    <DisplayName>AlarmAttempt</DisplayName>
    <PublisherDisplayName>mkrzeminski</PublisherDisplayName>
    <Logo>Assets\StoreLogo.png</Logo>
  </Properties>
  <Prerequisites>
    <OSMinVersion>6.3.1</OSMinVersion>
    <OSMaxVersionTested>6.3.1</OSMaxVersionTested>
